{
  "compilerOptions": {
    "baseUrl": ".",
    "checkJs": true,
    "module": "commonjs",
    "jsx": "react",
    "downlevelIteration": true,
    "paths": {
      "@/app/*": ["src/app/*"],
      "@/common/*": ["src/common/*"],
      "@/components/*": ["src/common/components/*"],
      "@/utils/*": ["src/common/utils/*"],
      "@/layouts/*": ["src/common/layouts/*"],
      "@/modules/*": ["src/modules/*"],
      "@/pages/*": ["src/pages/*"]
    },
  },
  "include": ["custom.d.ts"]
}
